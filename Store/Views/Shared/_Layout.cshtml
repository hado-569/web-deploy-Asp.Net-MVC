<!DOCTYPE HTML>
<html>
<head>
    <title>HaDo Store and More | Home </title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />



    <link href="~/assests/client/css/styleMain.css" rel="stylesheet" />
    <link href="~/assests/client/css/form.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/assests/client/css/fonts-googleapis.css" rel="stylesheet" />
    <link href="~/assests/client/css/back_to_top.css" rel="stylesheet" />
    <link href="~/assests/client/css/jquery-ui.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/assests/client/css/Style-media.css" rel="stylesheet" />

    

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>

    <!-- start menu -->
    <link href="~/assests/client/css/megamenu.css" rel="stylesheet" type="text/css" media="all" />
    <script type="text/javascript" src="~/assests/client/js/jquery1.min.js"></script>
    <script type="text/javascript" src="~/assests/client/js/megamenu.js"></script>

    <script>$(document).ready(function () { $(".megamenu").megamenu(); });</script>

    <!--start slider -->
    <link rel="stylesheet" href="~/assests/client/css/fwslider.css" media="all">
    <script src="~/assests/client/js/jquery-ui.min.js"></script>
    <script src="~/assests/client/js/css3-mediaqueries.js"></script>
    <script src="~/assests/client/js/fwslider.js"></script>

    <!--end slider -->

    <script src="~/assests/client/js/jquery.easydropdown.js"></script>

</head>
<body>
    <div class="header-top">
        @{ Html.RenderAction("TopMenu", "Home");}
    </div>
    <div class="header-bottom">
        <div class="wrap">
            <div class="header-bottom-left">
                <div class="logo">
                    <a href="/"><img src="/assests/client/images/logo.png" alt="" /></a>
                </div>

                @{Html.RenderAction("MainMenu", "Home"); }
            </div>
            @{Html.RenderAction("HeaderRight", "Home"); }
            <div class="clear"></div>
        </div>
    </div>
    <!-- start slider -->
    @RenderSection("slide", required: false)
    <!--/slider -->

    @RenderBody()
    <button onclick="topFunction()" id="myBtn" title="Go to top">Top</button>
   
    @{ Html.RenderAction("Footer", "Home");}
    <!--
    Java Script-->
    @*<script src="~/assests/client/js/Back_to_top.js"></script>
        <script src="~/assests/client/js/jquery-ui.js"></script>*@

    @Scripts.Render("~/bundles/jsController")
    @RenderSection("jsFooter", required: false)
</body>
</html>